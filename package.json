{
  "name": "deepseek-ai-chat",
  "displayName": "deepseek_ai_chat",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onView:chatView"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.11",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "DeepSeek AI Chat Settings",
      "properties": {
        "deepseek-ai-chat.bearerToken": {
          "type": "string",
          "description": "Bearer token acess API",
          "default": ""
        },
        "deepseek-ai-chat.promt": {
          "type": "string",
          "description": "Promt",
          "default": ""
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chat-container",
          "title": "DeepSeek",
          "icon": "resources/chat.svg"
        }
      ]
    },
    "views": {
      "chat-container": [
        {
          "type": "webview",
          "id": "chatView",
          "name": "Chat"
        }
      ]
    }
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "files": [
    "media/**",
    "out/**",
    "extension.js",
    "resources/**"
  ]
}
